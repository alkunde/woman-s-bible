{
  "expo": {
    "name": "Biblia para Elas",
    "slug": "womans-bible",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF699B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.gracetech.womansbible",
      "buildNumber": "17"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF699B"
      },
      "package": "br.com.gracetech.womansbible"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3200984351467142~5561666791",
          "iosAppId": "ca-app-pub-3200984351467142~2392076969",
          "delay_app_measurement_init": true,
          "user_tracking_usage_description": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53ac90d9-7423-403e-8caf-5085df66a88a"
      }
    }
  }
}
